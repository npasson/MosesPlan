{
  "manifest_version": 2,
  "name": "MosesPlan",
  "version": "0.1.1",
  "description": "Gives users the ability to add persistent custom events to 'Moseskonto' schedule of the Technical University of Berlin.",
  "content_scripts": [
    {
      "matches": [
        "*://moseskonto.tu-berlin.de/*"
      ],
      "js": [
        "lib/jquery-3.6.0.slim.js",
        "scripts/i18n.js",
        "scripts/event_manager.js",
        "scripts/render_manager.js",
        "scripts/popup_manager.js",
        "scripts/index.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  }
}