{
  "manifest_version": 2,
  "name": "MosesPlan",
  "version": "0.4",
  "description": "Add custom events to the TU Berlin 'Moses' timetable.",
  "content_scripts": [
    {
      "matches": [
        "*://moseskonto.tu-berlin.de/*"
      ],
      "js": [
        "lib/jquery-3.6.0.js",
        "scripts/global-styles.js",
        "scripts/i18n.js",
        "scripts/storage_manager.js",
        "scripts/settings_manager.js",
        "scripts/tutorial_manager.js",
        "scripts/render_helpers.js",
        "scripts/popover_manager.js",
        "scripts/render_manager.js",
        "scripts/popup_manager.js",
        "scripts/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "style/global.css"
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  }
}