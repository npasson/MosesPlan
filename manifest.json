{
  "manifest_version": 2,
  "name": "MosesPlan",
  "version": "0.4.2",
  "description": "Add custom events to the TU Berlin 'Moses' timetable.",
  "content_scripts": [
    {
      "matches": [
        "*://moseskonto.tu-berlin.de/*"
      ],
      "js": [
        "extension/lib/jquery-3.6.0.js",
        "extension/scripts/global-styles.js",
        "extension/scripts/misc_helpers.js",
        "extension/scripts/i18n.js",
        "extension/scripts/storage_manager.js",
        "extension/scripts/settings_manager.js",
        "extension/scripts/tutorial_manager.js",
        "extension/scripts/render_helpers.js",
        "extension/scripts/popover_manager.js",
        "extension/scripts/render_manager.js",
        "extension/scripts/popup_manager.js",
        "extension/scripts/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "extension/style/global.css"
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "extension/images/icon_16.png",
    "32": "extension/images/icon_32.png",
    "48": "extension/images/icon_48.png",
    "128": "extension/images/icon_128.png"
  }
}